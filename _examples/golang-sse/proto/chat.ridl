webrpc = v1

name = webrpc-sse-chat
version = v1.0.0

struct Message
  - id: uint64
  - text: string
  - authorName: string
  - createdAt: timestamp

service Chat
  - SendMessage(authorName: string, text: string)
  - SubscribeMessages(serverTimeoutSec: int) => stream (messages: Message)

error 100 ConnectionTerminated "connection terminated after 2mins" HTTP 400